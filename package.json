{
  "name": "express-server",
  "version": "1.0.0",
  "description": "scaffold for express server",
  "main": "./server/server.ts",
  "scripts": {
    "start": "node -r dotenv/config dist/server/server.js",
    "dev": "nodemon --watch 'server/**/*.ts' --exec ts-node -r dotenv/config server/server.ts",
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "telegram:webhook": "ts-node -r dotenv/config server/utils/telegramWebhookManager.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/core-auth": "^1.10.0",
    "@azure/functions": "^4.7.2",
    "@google-cloud/tasks": "^6.2.0",
    "@supabase/supabase-js": "^2.50.2",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "http-status-codes": "^2.3.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.13.1",
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.5",
    "twilio": "^5.7.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.30.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
